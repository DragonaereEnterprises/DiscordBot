{
  "name": "bot",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "prestart": "cd lavalink && pm2 start --name Lavalink java -- -jar Lavalink.jar",
    "start": "screen -dmS DragonaereDiscordBot bash -c 'node build/index.js | pino-pretty -i pid,hostname'",
    "start-dev": "node build/index.js | pino-pretty -i pid,hostname",
    "build": "tsc && pnpm sentry:sourcemaps",
    "stop-lavalink": "pm2 kill",
    "dev": "tsx watch index.ts",
    "sentry:sourcemaps": "sentry-cli sourcemaps inject --org dragonaereenterprises --project discord ./build && sentry-cli sourcemaps upload --org dragonaereenterprises --project discord ./build"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@andrewdragon/utils": "^1.0.0",
    "@apollo/server": "^4.10.1",
    "@sentry/cli": "^2.30.2",
    "cors": "^2.8.5",
    "discord.js": "~14.14.1",
    "dotenv": "^16.4.5",
    "express": "^4.18.3",
    "graphql": "^16.8.1",
    "lavalink-client": "^2.1.4",
    "ms": "^2.1.3",
    "pino": "^8.19.0",
    "pino-pretty": "^10.3.1",
    "reacord": "^0.6.0",
    "react": "^18.2.0"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/ms": "^0.7.34",
    "@types/node": "^20.11.28",
    "@types/react": "^18.2.66",
    "eslint": "^8.57.0",
    "tsup": "^8.0.2",
    "tsx": "^4.7.1",
    "typescript": "^5.4.2"
  }
}